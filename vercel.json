{
  "version": 2,
  "name": "my-weird-prompts",
  "builds": [
    {
      "src": "code/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/code/frontend/$1"
    }
  ]
}
